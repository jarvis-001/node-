<h1>{{title}}</h1>
<form action="api/members" method ="POST" class="mb-4">
<div class="form-group">
    <lable for ="name">Name</lable>
    <input type ="text" name ="name" class="form-control">
</div>
<div class="form-group">
    <lable for ="email">Email</lable>
    <input type ="email" name ="email" class="form-control">
</div>
<input type ="submit" value ="Add Member" class="btn btn-primary btn-block">
</form>
<ul class="list-group">
    {{#each members}}
    <l1 class ="list-group-item">{{this.name}}: {{this.email}}</l1>
    {{/each}}
</ul>

<a href="/api/members" class="btn btn-light">visit Api</a>